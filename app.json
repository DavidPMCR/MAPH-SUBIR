{
  "expo": {
    "name": "MAPH",
    "slug": "MAPH",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Se necesita acceso a la galería para seleccionar imágenes.",
        "NSPhotoLibraryAddUsageDescription": "Se necesita acceso para guardar archivos seleccionados.",
        "NSDocumentsFolderUsageDescription": "Se necesita acceso para seleccionar documentos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_MEDIA_LOCATION"
      ],
      "package": "com.davidpm.MAPH"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "897eaa54-1d29-42dc-9dc1-44a687b51707"
      }
    }
  }
}
